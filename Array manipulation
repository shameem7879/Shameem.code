public static void main(String[] args) {
    int[] rock = {1, 2, 3, 4, 5};
    
    // Reverse the array
    for (int i = 0; i < rock.length / 2; i++) {
        int temp = rock[i];
        rock[i] = rock[rock.length - 1 - i];
        rock[rock.length - 1 - i] = temp;
    }
    
    // Print the reversed array
    System.out.println(Arrays.toString(rock));
}
